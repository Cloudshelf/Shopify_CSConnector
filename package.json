{
  "name": "shopify_connector",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "--- dev options ---": "",
    "start:dev:env=local": "ENV_FILE_PATH=environment-variables/.local nest start --debug --watch",
    "start:dev:env=dev": "ENV_FILE_PATH=environment-variables/.dev nest start --debug --watch",
    "start:dev:env=prod": "ENV_FILE_PATH=environment-variables/.prod nest start --debug --watch",
    " ": "",
    "--- database options ---": "",
    "db:generate:migration": "ENV_FILE_PATH=environment-variables/.local MIKRO_ORM_CLI_CONFIG=src/modules/database/mikro-orm.config.ts yarn mikro-orm migration:create",
    "db:migrate:down": "ENV_FILE_PATH=environment-variables/.local MIKRO_ORM_CLI_CONFIG=src/modules/database/mikro-orm.config.ts yarn mikro-orm migration:down",
    "db:migrate:up": "ENV_FILE_PATH=environment-variables/.local MIKRO_ORM_CLI_CONFIG=src/modules/database/mikro-orm.config.ts yarn mikro-orm migration:up",
    "--- codegen options ---": "",
    "codegen:cloudshelf": "graphql-codegen --config src/graphql/cloudshelf/codegen.ts",
    "codegen:shopifyAdmin": "graphql-codegen --config src/graphql/shopifyAdmin/codegen.ts",
    "codegen:shopifyStorefront": "graphql-codegen --config src/graphql/shopifyStorefront/codegen.ts",
    "--- prod options ---": "",
    "prebuild": "rimraf dist",
    "build": "nest build && yarn sentry:sourcemaps",
    "start:prod": "node dist/main",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cloudshelf --project connector-shopify ./dist && sentry-cli --url https://apm.cloudshelf.ai sourcemaps upload --org cloudshelf --project connector-shopify ./dist",
    "--- other ---": "",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@apollo/server": "^4.10.0",
    "@mikro-orm/cli": "^6.0.5",
    "@mikro-orm/core": "^6.0.5",
    "@mikro-orm/migrations": "^6.0.5",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.0.5",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs-shopify/auth": "^4.2.0",
    "@nestjs-shopify/core": "^3.3.0",
    "@nestjs-shopify/webhooks": "^3.2.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/terminus": "^10.2.0",
    "@sentry/cli": "^2.25.3",
    "@sentry/node": "^7.94.1",
    "@sentry/profiling-node": "^1.3.5",
    "@sentry/tracing": "^7.94.1",
    "@shopify/shopify-api": "^9.0.2",
    "@slack/web-api": "^7.0.1",
    "apollo-server-core": "^3.13.0",
    "async": "^3.2.5",
    "date-fns": "^3.3.1",
    "dotenv-flow": "^4.1.0",
    "graphql": "^16.8.1",
    "graphql-relay": "^0.10.0",
    "http-proxy-middleware": "^2.0.6",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "nestjs-cls": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@ianvs/prettier-plugin-sort-imports": "3.7.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@shopify/app": "^3.53.1",
    "@shopify/cli": "^3.53.1",
    "@types/async": "^3.2.24",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.5.0",
    "patch-package": "^8.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
